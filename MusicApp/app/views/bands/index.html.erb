<h1 class='page-header'>Bands</h1>
<ul class='main-list'>
    <% @bands.each do |band| %>
        <li>
            <a href="<%= band_url(band) %>"><%= band.name %></a>
        </li>
        <% end %>
</ul>

<h4 class='sub-header'>Links</h4>
    <ul class='page-links'>
        <li><a class='button' href="<%= new_band_url %>">New Band</a>
        </li>
    
    </ul>